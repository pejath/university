<p id="notice"><%= notice %></p>


<p>
  <strong>Name:</strong>
  <%= @subject.name %>
</p>
<table>
  <thead>
    <tr>
      <th>Lecturers</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @subject.lecturer.map do |lecturer| %>
      <tr>
        <td><%= lecturer.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'Add lecturer', new_lecturers_subject_path%> |
<%= link_to 'Edit', edit_subject_path(@subject) %> |
<%= link_to 'Back', subjects_path %>
