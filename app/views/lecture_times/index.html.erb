<p id="notice"><%= notice %></p>

<h1>Lecture Times</h1>

<table>
  <thead>
    <tr>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lecture_times.each do |lecture_time| %>
      <tr>
        <td><%= lecture_time.formatted_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

